<?xml version="1.0" encoding="UTF-8"?>
<records>
    <record external_id="setting_space_low_level">
        <field name="label">Low level</field>
        <field name="icon">fa-wrench</field>
        <field name="description">Configure all the low level data</field>
        <field name="category" external_id="setting_space_category" />
        <field name="menus">
            <record external_id="menu_low_level_db">
                <field name="label">Database structure</field>
                <field name="position">left</field>
                <field name="children">
                    <record external_id="menu_low_level_db_model">
                        <field name="label">Models</field>
                        <field name="action" external_id="action_db_model"/>
                    </record>
                    <record external_id="menu_low_level_db_field">
                        <field name="label">Fields</field>
                        <field name="children">
                            <record external_id="menu_low_level_db_all_field">
                                <field name="label">All fields</field>
                                <field name="action" external_id="action_db_field"/>
                                <field name="order">1</field>
                            </record>
                            <record external_id="menu_low_level_db_column">
                                <field name="label">Columns</field>
                                <field name="action" external_id="action_db_column"/>
                                <field name="order">2</field>
                            </record>
                            <record external_id="menu_low_level_db_relationship">
                                <field name="label">Relation Ships</field>
                                <field name="action" external_id="action_db_rs"/>
                                <field name="order">3</field>
                            </record>
                        </field>
                    </record>
                </field>
            </record>
            <record external_id="menu_low_level_ui">
                <field name="label">Interface</field>
                <field name="position">left</field>
                <field name="children">
                    <record external_id="menu_low_level_ui_space">
                        <field name="label">Spaces and Categories</field>
                        <field name="children">
                            <record external_id="menu_low_level_ui_space_space">
                                <field name="label">Spaces</field>
                                <field name="action" external_id="action_ui_space"/>
                            </record>
                            <record external_id="menu_low_level_ui_space_category">
                                <field name="label">Spaces categories</field>
                                <field name="action" external_id="action_ui_space_category"/>
                            </record>
                        </field>
                        <field name="order">1</field>
                    </record>
                    <record external_id="menu_low_level_ui_menu">
                        <field name="label">Menus</field>
                        <field name="action" external_id="action_ui_menu"/>
                        <field name="order">2</field>
                    </record>
                    <record external_id="menu_low_level_ui_action">
                        <field name="label">Actions</field>
                        <field name="action" external_id="action_ui_action"/>
                        <field name="order">3</field>
                    </record>
                    <record external_id="menu_low_level_ui_view">
                        <field name="label">Views</field>
                        <field name="children">
                            <record external_id="menu_low_level_ui_all_view">
                                <field name="label">All views</field>
                                <field name="action" external_id="action_ui_all_view"/>
                            </record>
                            <record external_id="menu_low_level_ui_list_view">
                                <field name="label">List views</field>
                                <field name="action" external_id="action_ui_list_view"/>
                            </record>
                            <record external_id="menu_low_level_ui_thumbnail_view">
                                <field name="label">Thumbnail views</field>
                                <field name="action" external_id="action_ui_thumbnail_view"/>
                            </record>
                            <record external_id="menu_low_level_ui_form_view">
                                <field name="label">Form views</field>
                                <field name="action" external_id="action_ui_form_view"/>
                            </record>
                        </field>
                        <field name="order">4</field>
                    </record>
                </field>
            </record>
            <record external_id="menu_low_level_io">
                <field name="label">In / Out</field>
                <field name="position">left</field>
                <field name="children">
                    <record external_id="menu_low_level_io_mapper">
                        <field name="label">Mappings</field>
                        <field name="action" external_id="action_io_mapping"/>
                    </record>
                    <record external_id="menu_low_level_io_importer">
                        <field name="label">Importers</field>
                        <field name="action" external_id="action_io_importer"/>
                    </record>
                    <record external_id="menu_low_level_io_exporter">
                        <field name="label">Exporters</field>
                        <field name="action" external_id="action_io_exporter"/>
                    </record>
                </field>
            </record>
        </field>
        <field name="default_menu" external_id="menu_low_level_db_model" />
    </record>
</records>
